{
  "name": "gatsby-starter-location-github",
  "description": "Local GitHub directory.",
  "version": "0.0.0",
  "dependencies": {
    "gatsby": "next",
    "gatsby-plugin-react-helmet": "next",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0"
  },
  "keywords": ["gatsby", "starter", "github"],
  "lint-staged": {
    "*.{js,json,md}": ["prettier-standard", "git add"]
  },
  "scripts": {
    "build": "gatsby build",
    "serve": "gatsby serve",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier-standard '**/*.+(js|json|md)'",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "bootstrap": "^4.1.2",
    "gatsby-plugin-sass": "^2.0.0-beta.5",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.9.2",
    "prettier-standard": "^8.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
